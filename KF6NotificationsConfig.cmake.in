@PACKAGE_INIT@

set(KNOTIFICATIONS_DBUS_INTERFACES_DIR "@PACKAGE_KDE_INSTALL_DBUSINTERFACEDIR@")

include(CMakeFindDependencyMacro)
find_dependency(Qt6Gui @REQUIRED_QT_VERSION@)
if (@HAVE_WIDGETS@)
    find_dependency(Qt6Widgets @REQUIRED_QT_VERSION@)
endif()
if (@HAVE_DBUS@)
    find_dependency(Qt6DBus @REQUIRED_QT_VERSION@)
endif()

if (NOT @BUILD_SHARED_LIBS@) 
    find_dependency(KF6CoreAddons "@KF_DEP_VERSION@")
    find_dependency(KF6Config "@KF_DEP_VERSION@")

    if (@HAVE_KWINDOWSYSTEM@)
        find_dependency(KF6WindowSystem "@KF_DEP_VERSION@")
    endif()

    if (@WITH_SNORETOAST@)
        find_dependency(LibSnoreToast)
        find_dependency(Qt6Network @REQUIRED_QT_VERSION@)
    endif()

    if (@HAVE_PHONON4QT6@)
        find_dependency(Phonon4Qt6)
    endif()

    if (@Canberra_FOUND@)
        find_dependency(Canberra)
    endif()

    if (@Qt6TextToSpeech_FOUND@)
        find_dependency(Qt6TextToSpeech @REQUIRED_QT_VERSION@)
    endif()

    if(@HAVE_DBUSMENUQT@)
        find_dependency(dbusmenu-qt6)
    endif()
endif()

include("${CMAKE_CURRENT_LIST_DIR}/KF6NotificationsTargets.cmake")
@PACKAGE_INCLUDE_QCHTARGETS@
